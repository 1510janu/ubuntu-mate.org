---
layout: null
---
[
  {% assign posts = site.posts | concat: site.pages %}
  {% for post in posts %}
    {% if post.title == null %}{% continue %}{% endif %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.category }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    },
  {% endfor %}
  {}
]
